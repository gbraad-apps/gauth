{
  "manifest_version": 2,

  "name": "GAuth Authenticator",
  "description": "A simple application for multi-factor authentication",
  "version": "1.0",

  "permissions": [
    "storage",
    "clipboardWrite"
  ],
  "browser_action": {
    "default_icon": "img/icon_512.png",
    "default_popup": "index.html"
  }
}